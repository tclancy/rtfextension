{
  "name": "RTF Viewer",
  "version": "0.1",
  "description": "Allows viewing RTF directly inside Chrome.",
  "manifest_version": 2,
  "icons": {
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ]
}