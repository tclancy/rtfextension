{
  "manifest_version": 2,
  "name": "RTF Reader",
  "version": "0.1",
  "permissions": [
    "activeTab"
   ],
  "content_scripts": [
    {
      "matches": [
        "https?://*/*.rtf"
      ],
      "js": [
        "jquery-2.2.0.min.js",
        "rtf.js",
        "reader.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}